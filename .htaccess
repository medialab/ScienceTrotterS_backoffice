RewriteEngine On

RewriteRule	^([a-z0-9/\.-]+)\.(css|js)$ /?name=$1&extension=$2&test=2%1 [L]
RewriteRule ^lib/([a-z0-9/\.-]+)\.(.*)$ /?name=$1&extension=$2&test=1%1 [L]

RewriteRule ^edit/([a-z-]+)/([^@]+)\.html$ /?name=edit/$1&extension=html&id=$2%1 [L]

RewriteRule ^edit/([a-z-]+)/@(.*)\.html$ /?name=edit/$1&extension=html&parent=$2%1 [L]


RewriteRule ^delete/([a-z-]+)/(.*)\.html$ /?name=delete/$1&extension=html&id=$2%1 [L]
RewriteRule ^upload/([a-z0-9/.-]+)$ /?name=upload&extension=html&hash=$1%1 [L]

RewriteRule index\.(html|admin|popup)$ /?name=index&extension=$1%1 [L]
RewriteRule (.*)\.(html|admin|popup)$ /?name=$1&extension=$2%1 [L]